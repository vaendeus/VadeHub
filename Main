local GameScripts = {
    [18799085098] = "https://raw.githubusercontent.com/vaendeus/VadeHubScripts/main/HideorDie"
}

local ScriptURL = GameScripts[game.PlaceId]

if ScriptURL then
    loadstring(game:HttpGet(ScriptURL, true))()
else
    loadstring(game:HttpGet("YOUR_HUB_URL_HERE", true))() -- Replace with your hub loader
end
